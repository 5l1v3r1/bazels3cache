{
    "defaultSeverity": "error",
    "extends": ["tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "adjacent-overload-signatures": true,
        "align": false,
        "array-type": false,
        "arrow-parens": false,
        "arrow-return-shorthand": true,
        "await-promise": false,
        "ban": [
            true,
            ["assert", "equal", "You must use assert.strictEqual"],
            ["assert", "notEqual", "You must use assert.notStrictEqual"],
            ["event", "ctrlKey", "You must use metaOrCtrl or NavigationLink to handle ctrlKey"],
            ["event", "metaKey", "You must use metaOrCtrl or NavigationLink to handle metaKey"],
            ["sandbox", "create", "You must use the withSandbox function to create a sandbox"],
            ["sinon", "stub", "You must use a sandbox to create a stub"],
            ["sinon", "spy", "You must use a sandbox to create a spy"]
        ],
        "ban-types": false,
        "callable-types": false,
        "class-name": true,
        "comment-format": [true, "check-space"],
        "completed-docs": false,
        "curly": true,
        "cyclomatic-complexity": false,
        "eofline": true,
        "forin": true,
        "import-blacklist": false,
        "import-spacing": false,
        "indent": [true, "spaces"],
        "interface-name": false,
        "import-ordering": false,
        "interface-over-type-literal": false,
        "jsdoc-format": false,
        "label-position": true,
        "match-default-export-name": false,
        "max-classes-per-file": false,
        "max-line-length": false,
        "member-access": false,
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "new-parens": true,
        "newline-before-return": false,
        "no-angle-bracket-type-assertion": false,
        "no-any": false,
        "no-arg": true,
        "no-bitwise": true,
        "no-boolean-literal-compare": false,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-console": false,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-super": true,
        "no-duplicate-variable": true,
        "no-empty": false,
        "no-empty-interface": false,
        "no-eval": true,
        "no-floating-promises": false,
        "no-import-side-effect": true,
        "no-inferred-empty-object-type": false,
        "no-inferrable-types": true,
        "no-internal-module": true,
        "no-invalid-this": true,
        "no-magic-numbers": false,
        "no-misused-new": false,
        "no-namespace": true,
        "no-non-null-assertion": false,
        "no-null-keyword": false,
        "no-parameter-properties": false,
        "no-reference": true,
        "no-require-imports": true,
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-string-throw": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unbound-method": false,
        "no-unnecessary-initializer": false,
        "no-unnecessary-qualifier": false,
        "no-unsafe-any": false,
        "no-unsafe-finally": true,
        "no-unused-expression": true,
        "no-unused-variable": true,
        "no-use-before-declare": false,
        "no-var-keyword": true,
        "no-var-requires": true,
        "no-void-expression": false,
        "object-literal-key-quotes": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "one-line": false,
        "one-variable-per-declaration": [true, "ignore-for-loop"],
        "only-arrow-functions": false,
        "ordered-imports": false,
        "prefer-const": true,
        "prefer-for-of": false,
        "prefer-function-over-method": false,
        "prefer-method-signature": false,
        "quotemark": [true, "avoid-escape", "double"],
        "radix": true,
        "semicolon": [true, "always", "ignore-bound-class-methods"],
        "space-before-function-paren": [true, "never"],
        "strict-boolean-expressions": false,
        "strict-type-predicates": false,
        "switch-default": false,
        "trailing-comma": [
            true,
            {
                "singleline": "never",
                "multiline": "never"
            }
        ],
        "triple-equals": true,
        "typedef": [true, "property-declaration"],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            },
            {
                "call-signature": "space",
                "index-signature": "space",
                "parameter": "space",
                "property-declaration": "space",
                "variable-declaration": "space"
            }
        ],
        "typeof-compare": true,
        "unified-signatures": true,
        "use-isnan": true,
        "variable-name": [true, "allow-leading-underscore", "allow-pascal-case", "ban-keywords"],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type"
        ]
    },
    "rulesDirectory": []
}
